{
  "components/order-summary": "<article class=\"plp-order-summary\" >\r\n    <div data-plp-cart>\r\n        <header>\r\n            <h3>Your Cart</h3>\r\n            <hr style=\"width:100%;text-align:left;margin-left:0\">\r\n            <div id=\"plp-order-summary-products\">No product found in cart</div>\r\n\r\n            <!-- Add to Cart with SKU - START -->\r\n            <hr style=\"width:100%;text-align:left;margin-left:0\">\r\n            <table class=\"plp-order-summary-add-sku-to-cart\">\r\n                <tr width=\"100%\">\r\n                    <td width=\"50%\" style=\"padding:2px;\">\r\n                        <input type=\"text\" size=\"20\" placeholder=\"Quick add SKU\" data-id=\"quick-add-item-sku\"></input>\r\n\r\n                        <!-- Display SKU Search Results - START  -->\r\n                        <div class=\"items-search-results-container\" data-id=\"sku-search-results-container\">\r\n                            <!-- <ul class=\"items-search-results\" data-id=\"sku-search-results\">\r\n                                <li>ABS</li>\r\n                                <li>DPB</li>\r\n                            </ul> -->\r\n                        </div>\r\n                        <!-- Display SKU Search Results - END  -->\r\n                        \r\n                    </td>\r\n                    <td width=\"30%\" style=\"padding:2px;\">\r\n                        x <input type=\"number\" min='0' max='999' data-id=\"quick-add-item-qty\" />\r\n                    </td>\r\n                    <td align=\"left\" width=\"20%\" style=\"padding:2px;\">\r\n                        <button class=\"quick-add-sku-button\" data-class=\"quick-add-sku-button\"><strong data-class=\"quick-add-sku-button\">+</strong></button>\r\n                    </td>\r\n                </tr>\r\n                <!-- Display SKU Search Results - START  -->\r\n                <tr width=\"100%\">\r\n                    <td width=\"50%\" style=\"padding:2px;\">\r\n                        <button class=\"quick-add-sku-button\" data-class=\"search-matching-sku-results-btn\">Search</button>\r\n                    </td>\r\n                    <td width=\"30%\" style=\"padding:2px;\"></td>\r\n                    <td align=\"left\" width=\"20%\" style=\"padding:2px;\"></td>\r\n                </tr>\r\n                <!-- Display SKU Search Results - END  -->\r\n            </table>\r\n            <!-- Add to Cart with SKU - END -->\r\n            <hr style=\"width:100%;text-align:left;margin-left:0\">\r\n            <table class=\"plp-order-summary-table\" style=\"display:block;\">\r\n                <tr class=\"plp-order-summary-row\">\r\n                    <td width=\"100%\">Total Items:</td>\r\n                    <td width=\"100%\" id=\"api-total-items\" class=\"plp-order-summary-column-value\">\r\n                        <span>0</span>\r\n                    </td> \r\n                </tr>\r\n                <tr class=\"plp-order-summary-row\">\r\n                    <td width=\"100%\">Subtotal:</td>\r\n                    <td width=\"100%\" class=\"plp-order-summary-column-value\">\r\n                        {{settings.money.currency_token}}<span id=\"api-subtotal\">0.00</span>\r\n                    </td>\r\n                </tr>\r\n                <tr class=\"plp-order-summary-row\">\r\n                    <td width=\"100%\">Total Tax:</td>\r\n                    <td width=\"100%\" class=\"plp-order-summary-column-value\">\r\n                        {{settings.money.currency_token}}<span id=\"api-taxtotal\">0.00</span>\r\n                    </td>\r\n                </tr>\r\n                <tr class=\"plp-order-summary-row\">\r\n                    <td width=\"100%\">Grand Total:</td>\r\n                    <td width=\"100%\" class=\"plp-order-summary-column-value\">\r\n                        {{settings.money.currency_token}}<span id=\"api-grandtotal\">0.00</span>\r\n                    </td>\r\n                </tr>\r\n            </table>\r\n        </header>\r\n        <div class=\"loadingOverlay\" style=\"display: none;\"></div>\r\n    </div>\r\n</article>"
}
